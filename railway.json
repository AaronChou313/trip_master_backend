{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "node server.js",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300
  },
  "env": {
    "PORT": {
      "value": "3000",
      "fromServiceId": false
    }
  }
}